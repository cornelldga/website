  /* Styles for the PokÃ©mon card */
.pokemon-card {
    border: 2px solid black;
    width: 300px;
    padding: 15px;
    margin: 20px;
    display: flex;
}

.card-image {
    border: 1px solid black;
    margin-right: 10px;
}

.card-image img {
    width: 100%;
    height: auto;
}

.card-info {
    display: flex;
    flex-direction: column;
}

.caption {
    background-color: black;
    color: white;
    padding: 5px;
    margin-bottom: 10px;
}

.power-description {
    margin-bottom: 10px;
}

.moves {
    border-top: 1px solid black;
    margin-top: 10px;
}

.move {
    display: flex;
    justify-content: space-between;
    padding: 5px;
}

.rarity {
    margin-top: 10px;
}
